{
  "name": "custom-vue-template",
  "version": "0.0.1",
  "description": "My Custom Vue template",
  "scripts": {
    "format": "prettier --write src/",
    "lint": "eslint src/ --ext .ts,.vue",
    "test": "test",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-toscani/custom-vue-template.git"
  },
  "keywords": [
    "Template"
  ],
  "author": "Julian Toscani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-toscani/custom-vue-template/issues"
  },
  "homepage": "https://github.com/j-toscani/custom-vue-template#readme",
  "dependencies": {
    "vue": "^3.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vue-eslint-parser": "^7.10.0",
    "vue-tsc": "^0.2.3"
  }
}
